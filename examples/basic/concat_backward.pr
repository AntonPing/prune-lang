datatype IntList where
| Cons(Int, IntList)
| Nil
end

function concat(xs: IntList, ys: IntList) -> IntList
begin
    match xs with
    | Cons(head, tail) => Cons(head, concat(tail, ys))
    | Nil => ys
    end
end

function example(xs: IntList, ys: IntList)
begin
    guard concat(xs, ys) = Cons(1, Cons(2, Cons(3, Cons(4, Nil))));
end

query example(depth_step=50, depth_limit=1000, answer_limit=5)